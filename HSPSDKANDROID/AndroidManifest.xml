<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.hangame.hsp">

    <application>

        <!-- [HSP SDK] kakao login activity start -->

        <activity android:configChanges="orientation|keyboardHidden|screenSize" android:name="com.hangame.hsp.auth.login.kakao.KakaoLoginActivity" android:theme="@android:style/Theme.Translucent.NoTitleBar">
        </activity>

        <!-- [HSP SDK] kakao login activity end -->
    

        <!-- [HSP SDK] Activity Begin -->
        <activity android:configChanges="orientation|screenSize|keyboardHidden" android:finishOnTaskLaunch="true" android:name="com.hangame.hsp.ui.HSPUiActivity" android:theme="@android:style/Theme.Translucent.NoTitleBar" android:windowSoftInputMode="adjustPan">
        </activity>
        <!-- [HSP SDK] Activity End -->

    </application>



    <!-- [HSP SDK] min sdk version begin -->
    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="17"/>
    <!-- [HSP SDK] min sdk version end -->


    <!-- [HSP SDK] permission begin -->
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <!-- [HSP SDK] permission end -->

    



    <!-- ﻿[HSP SDK] Payment Google Play Billing Permission Begin -->
    <!-- 구글 마켓 빌드시에만 사용해주세요. -->
    <uses-permission android:name="com.android.vending.BILLING"/>
    <!-- ﻿[HSP SDK] Payment Google Play Billing Permission End -->

	


</manifest>